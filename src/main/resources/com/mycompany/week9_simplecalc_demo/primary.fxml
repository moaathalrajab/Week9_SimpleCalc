<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.week9_simplecalc_demo.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <HBox prefHeight="52.0" prefWidth="200.0">
         <children>
            <TextField fx:id="tf_Output" alignment="CENTER_RIGHT" prefHeight="56.0" prefWidth="200.0" promptText="0">
               <font>
                  <Font name="System Bold" size="26.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <HBox prefHeight="32.0" prefWidth="200.0">
         <children>
            <Button minWidth="45.0" mnemonicParsing="false" onAction="#processDigit" text="7">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button minWidth="45.0" mnemonicParsing="false" onAction="#processDigit" text="8">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button minWidth="45.0" mnemonicParsing="false" onAction="#processDigit" text="9">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnDvd" minWidth="45.0" mnemonicParsing="false" onAction="#processArthOp" text="/">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="37.0" prefWidth="200.0">
         <children>
            <Button minWidth="45.0" mnemonicParsing="false" onAction="#processDigit" text="4">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button minWidth="45.0" mnemonicParsing="false" onAction="#processDigit" text="5">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button minWidth="45.0" mnemonicParsing="false" onAction="#processDigit" text="6">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnMlt" minWidth="45.0" mnemonicParsing="false" onAction="#processArthOp" text="x">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="29.0" prefWidth="200.0">
         <children>
            <Button minWidth="45.0" mnemonicParsing="false" onAction="#processDigit" text="1">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button minWidth="45.0" mnemonicParsing="false" onAction="#processDigit" text="2">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button minWidth="45.0" mnemonicParsing="false" onAction="#processDigit" text="3">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnSub" minWidth="45.0" mnemonicParsing="false" onAction="#processArthOp" text="-">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="34.0" prefWidth="200.0">
         <children>
            <Button minWidth="45.0" mnemonicParsing="false" text=" ">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button minWidth="45.0" mnemonicParsing="false" onAction="#processDigit" text="0">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnEql" minWidth="45.0" mnemonicParsing="false" onAction="#processEqual" text=" =">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnAdd" minWidth="45.0" mnemonicParsing="false" onAction="#processArthOp" text="+">
               <HBox.margin>
                  <Insets right="6.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
